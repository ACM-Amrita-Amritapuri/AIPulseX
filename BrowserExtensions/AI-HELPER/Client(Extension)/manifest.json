{
    "manifest_version": 3,
    "name": "AI Helper",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
